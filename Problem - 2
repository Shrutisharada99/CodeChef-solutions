# Python

T = int(input())
for i in range(T):
    x,y,a = input().split(' ')
    if x<=a<y:
        print("YES")
    else:
        print("NO")

// C++

#include <iostream>
using namespace std;
int main() {
    int T = 0, x = 0, y = 0, a = 0;
    cin>>T;
    while(T--) {
        cin>>x>>y>>a;
        if (x<=a && a<y)
            cout<<"YES\n";
        else
            cout<<"NO\n";
    }
	// your code goes here
	return 0;
}

// C

#include <stdio.h>

int main(void) {
	// your code goes here
    int T = 0, X = 0, Y = 0, A = 0;
    scanf("%d", &T);
    while(T--) {
        scanf("%d %d %d", &X, &Y, &A);
        if((X <= A) && (A < Y))
            printf("%s\n","YES");
        else
            printf("%s\n","NO");
    }
	return 0;
}
